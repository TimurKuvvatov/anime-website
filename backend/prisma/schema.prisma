generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url = env("POSTGRES_URL")
    directUrl = env("POSTGRES_URL_NON_POOLING")
}

model News {
    id Int @id @default(autoincrement())

    title   String
    email   String
    content String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
